<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <com.hyphenate.easeui.widget.EaseTitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#232325"
        app:titleBarLeftImage="@drawable/ease_mm_title_back" />
<include layout="@layout/ease_search_bar_with_padding" android:id="@+id/search_bar_view"/>
  <FrameLayout
      android:id="@+id/cotent_container"
      android:layout_width="match_parent"
      android:layout_height="match_parent">
      <com.hyphenate.easeui.widget.EaseContactList
          android:id="@+id/contact_list"
          android:layout_width="match_parent"
          android:layout_height="match_parent">

      </com.hyphenate.easeui.widget.EaseContactList>

  </FrameLayout>

</LinearLayout>
